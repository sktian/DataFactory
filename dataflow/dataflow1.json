{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "WWIODS",
						"type": "DatasetReference"
					},
					"name": "Orders",
					"description": "Import data from ODS"
				},
				{
					"dataset": {
						"referenceName": "WWIODS",
						"type": "DatasetReference"
					},
					"name": "OrderLines"
				},
				{
					"name": "Customers"
				},
				{
					"name": "Invoices"
				},
				{
					"name": "People"
				},
				{
					"name": "PackageTYPE"
				},
				{
					"name": "StockItems"
				}
			],
			"sinks": [
				{
					"name": "ExporttoDW"
				}
			],
			"transformations": [
				{
					"name": "joinOrdersOrderlines"
				},
				{
					"name": "JoinwithCustomersTable"
				},
				{
					"name": "JoinwithInvoices"
				},
				{
					"name": "JoinwithPeople"
				},
				{
					"name": "joinwithptype"
				},
				{
					"name": "joinwithstock"
				}
			],
			"scriptLines": [
				"source(output(",
				"          OrderID as integer,",
				"          CustomerID as integer,",
				"          SalespersonPersonID as integer,",
				"          PickedByPersonID as integer,",
				"          ContactPersonID as integer,",
				"          BackorderOrderID as integer,",
				"          OrderDate as date,",
				"          ExpectedDeliveryDate as date,",
				"          CustomerPurchaseOrderNumber as string,",
				"          IsUndersupplyBackordered as boolean,",
				"          Comments as string,",
				"          DeliveryInstructions as string,",
				"          InternalComments as string,",
				"          PickingCompletedWhen as timestamp,",
				"          LastEditedBy as integer,",
				"          LastEditedWhen as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT *\\nFROM Sales.Orders',",
				"     format: 'query',",
				"     wildcardPaths:['Sales.Orderlines.txt']) ~> Orders",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 100,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT *\\nFROM Sales.OrderLines',",
				"     format: 'query',",
				"     wildcardPaths:['Sales.Orders.txt']) ~> OrderLines",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> Customers",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> Invoices",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> People",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> PackageTYPE",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> StockItems",
				"Orders, OrderLines join(fuzzyCompare(ERROR_FUNCTION(''), ERROR_FUNCTION(''), 71.00),",
				"     joinType:'inner',",
				"     matchType:'fuzzy',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinOrdersOrderlines",
				"joinOrdersOrderlines, Customers join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> JoinwithCustomersTable",
				"JoinwithCustomersTable, Invoices join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> JoinwithInvoices",
				"JoinwithInvoices, People join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> JoinwithPeople",
				"JoinwithPeople, PackageTYPE join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinwithptype",
				"joinwithptype, StockItems join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinwithstock",
				"joinwithstock sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     format: 'table') ~> ExporttoDW"
			]
		}
	}
}