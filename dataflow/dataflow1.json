					},
					"name": "Customers"
				},
				{
					"dataset": {
						"referenceName": "WWI_ODS",
						"type": "DatasetReference"
					},
					"name": "Invoices"
				},
				{
					"dataset": {
						"referenceName": "WWI_ODS",
						"type": "DatasetReference"
					},
					"name": "People"
				},
				{
